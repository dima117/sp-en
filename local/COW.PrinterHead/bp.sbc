public virtual void MoveFromCockpit()
{
isPressd = false;
StopMoving?.Invoke();
foreach (IMyShipController ck in ctrlList)
{
Vector3 mi = ck.MoveIndicator;
if (mi.Length() &gt; 0.173)
{
isPressd = true;
if (mi.X &gt; 0.1) StartMoving?.Invoke(2, CurrPower);
else if (mi.X &lt; -0.1) StartMoving?.Invoke(1, CurrPower);
if (mi.Y &gt; 0.1) StartMoving?.Invoke(3, CurrPower);
else if (mi.Y &lt; -0.1) StartMoving?.Invoke(4, CurrPower);
if (mi.Z &gt; 0.1) StartMoving?.Invoke(6, CurrPower);
else if (mi.Z &lt; -0.1) StartMoving?.Invoke(5, CurrPower);
}
}
GGyros.ProcessGyrosStab(isPressd);
}
public virtual void DamperMove(double minspeed)
{
if (CurrController.DampenersOverride &amp;&amp; !isPressd)
{
Vector3I dirSpd = CalcDirSpd(CurrController);
if (isSPM) minspeed = minspeed / 4;
float cp = CurrPower;
if (!isCruise &amp;&amp; dirSpd.X &gt; minspeed)
{
if (dirSpd.X &lt; 2 * minspeed) cp = slowPowerLvl;
StartMoving?.Invoke(6, cp);
GGyros.ProcessGyrosStab(true);
}
else if (dirSpd.X &lt; -minspeed)
{
if (dirSpd.X &gt; 2 * minspeed) cp = slowPowerLvl;
StartMoving?.Invoke(5, cp);
GGyros.ProcessGyrosStab(true);
}
if (dirSpd.Y &gt; minspeed)
{
if (dirSpd.Y &lt; 2 * minspeed) cp = slowPowerLvl;
StartMoving?.Invoke(2, cp);
GGyros.ProcessGyrosStab(true);
}
else if (dirSpd.Y &lt; -minspeed)
{
if (dirSpd.Y &gt; 2 * minspeed) cp = slowPowerLvl;
StartMoving?.Invoke(1, cp);
GGyros.ProcessGyrosStab(true);
}
if (dirSpd.Z &gt; minspeed)
{
if (dirSpd.Z &lt; 2 * minspeed) cp = slowPowerLvl;
StartMoving?.Invoke(4, cp);
GGyros.ProcessGyrosStab(true);
}
else if (dirSpd.Z &lt; -minspeed)
{
if (dirSpd.Z &gt; 2 * minspeed) cp = slowPowerLvl;
StartMoving?.Invoke(3, cp);
GGyros.ProcessGyrosStab(true);
}
}
}
public virtual void EvasionMove()
{
if (GGyros.Mode == DGyros.GMode.evasion)
{
StartMoving?.Invoke(3, G);
isPressd = true;
}
}
protected List&lt;T&gt; UniteLists&lt;T&gt;(List&lt;T&gt;[] tLists)
{
var tmp = new List&lt;T&gt;();
for (int i = 0; i &lt; tLists.Length; i++) tmp = tmp.Union(tLists[i]).ToList();
return tmp;
}
protected void SetBlocksOnOff&lt;T&gt;(List&lt;T&gt; blockList, bool onoff) where T : IMyFunctionalBlock
{
foreach (IMyFunctionalBlock bl in blockList) bl.Enabled = onoff;
}
protected Vector3I CalcDirSpd(IMyShipController ctrl)
{
MyShipVelocities shipVel = ctrl.GetShipVelocities();
Vector3D vForw = ctrl.WorldMatrix.Forward;
Vector3D vLeft = ctrl.WorldMatrix.Left;
Vector3D vUp = ctrl.WorldMatrix.Up;
float spdForw = (float)vForw.Dot(shipVel.LinearVelocity);
float spdLeft = (float)vLeft.Dot(shipVel.LinearVelocity);
float spdUp = (float)vUp.Dot(shipVel.LinearVelocity);
return new Vector3I(spdForw, spdLeft, spdUp);
}
public static IMyShipController GetShipController(List&lt;IMyShipController&gt; ctrls)
{
foreach (IMyShipController c in ctrls)
{
if (c.IsUnderControl &amp;&amp; c.CanControlShip) return c;
}
return ctrls[0];
}
}
public class SphericalGDrive : CommonGDrive, IGDrive
{
public SphericalGDrive(ref List&lt;IMyFunctionalBlock&gt; gravList, ref List&lt;IMyFunctionalBlock&gt;[] massLists, List&lt;IMyShipController&gt; ctrls, float slowpow, float maxFieldSile, float slowFieldSize, DThrusters gThrusters, DGyros gGyros)
: base(ctrls, slowpow, maxFieldSile, slowFieldSize, gThrusters, gGyros)
{
this.centralList = gravList;
this.sideLists = massLists;
this.sideLists[0] = UniteLists(massLists);
massLists[0] = this.sideLists[0];
Enabled = false;
Cruise = false;
SPM = false;
StartMoving += SphericalGravMove;
StopMoving += SphericalStop;
}
public override bool Enabled
{
get { return base.Enabled; }
set
{
base.Enabled = value;
SetBlocksOnOff(centralList, Enabled);
if (!Enabled)
{
if (GGyros.Mode == DGyros.GMode.evasion) GGyros.ToggleEvasion();
GThrusters.ToggleEvasion(value);
SetBlocksOnOff(sideLists[0], false);
}
}
}
public override bool SPM
{
get { return base.SPM; }
set
{
isSPM = value;
CurrPower = isSPM ? slowPowerLvl : G;
if (isSPM) SetGravFieldAndPower(centralList, slowFieldSize, slowPowerLvl);
else SetGravFieldAndPower(centralList, maxFieldSile, G);
}
}
private void SphericalGravMove(int dir, float pow)
{
SetBlocksOnOff(sideLists[dir], true);
