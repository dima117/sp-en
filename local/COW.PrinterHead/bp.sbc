public class LinearGDrive : CommonGDrive, IGDrive
{
public LinearGDrive(ref List&lt;IMyFunctionalBlock&gt; massList, ref List&lt;IMyFunctionalBlock&gt;[] gravLists, List&lt;IMyShipController&gt; ctrls, float slowpow, float maxFieldSile, float slowFieldSize, DThrusters gThrusters, DGyros gGyros)
