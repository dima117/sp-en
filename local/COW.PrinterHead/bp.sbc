public class INI
{
private readonly IMyTerminalBlock iniBlock;
public INI(IMyTerminalBlock block)
{
iniBlock = block;
}
public bool Read&lt;T&gt;(string section, string key, ref T Variable, T defaultvalue, bool trim = true)
{
string temp = "";
if (ReadKey(section, key, ref temp, trim))
{
Variable = (T)Convert.ChangeType(temp, typeof(T));
return true;
}
else
{
Variable = defaultvalue;
return false;
}
}
public bool Read(string section, string key, ref Color Variable, Color defaultvalue, bool Trim = true)
{
string temp = "";
if (ReadKey(section, key, ref temp, Trim))
{
string[] tmp = temp.Split(',');
byte r, g, b;
if (Byte.TryParse(tmp[0], out r) &amp;&amp; Byte.TryParse(tmp[1], out g) &amp;&amp; Byte.TryParse(tmp[2], out b))
{
Variable.R = r;
Variable.G = g;
Variable.B = b;
return true;
}
else
{
Variable = defaultvalue;
return false;
}
}
else return false;
}
private bool ReadKey(string section, string key, ref string Variable, bool trim)
{
string iniData = iniBlock.CustomData;
var datalines = iniData.Split('\n');
string currsect = "";
foreach (string line in datalines)
{
string trimline = line.Trim();
if (trimline.StartsWith("[") &amp;&amp; trimline.EndsWith("]"))
{
currsect = trimline;
}
else if (currsect.Equals($"[{section}]"))
{
string[] lineparts = line.Split(new[] { '=' }, 2);
if (lineparts.Length &gt;= 1 &amp;&amp; lineparts[0] == key)
{
if (lineparts.Length &gt;= 2)
{
if (trim) Variable = lineparts[1].Trim();
else Variable = lineparts[1];
return true;
}
else return false;
}
}
}
return false;
}
public string ReadData()
{
return iniBlock.CustomData;
}
public bool FindSection(string section)
{
string iniData = iniBlock.CustomData;
var datalines = iniData.Split('\n');
foreach (string line in datalines)
{
string trimline = line.Trim();
if (trimline.StartsWith("[") &amp;&amp; trimline.EndsWith("]") &amp;&amp; System.Text.RegularExpressions.Regex.IsMatch(trimline, section, System.Text.RegularExpressions.RegexOptions.IgnoreCase))
{
return true;
}
}
return false;
}
public void ClearCustomData()
{
iniBlock.CustomData = "";
}
public void AddSection(string section)
{
if (iniBlock.CustomData == "") iniBlock.CustomData += $"[{section}]";
else iniBlock.CustomData += $"\n\n[{section}]";
}
public void Write(string section, string key, string value)
{
string iniData = iniBlock.CustomData;
var datalines = iniData.Split('\n');
string currsect = "";
int pos = -1;
foreach (string line in datalines)
{
pos = pos + line.Length + 1;
string trimline = line.Trim();
if (trimline.StartsWith("[") &amp;&amp; trimline.EndsWith("]")) currsect = trimline;
if (currsect.Equals($"[{section}]"))
{
iniData = iniData.Insert(pos, $"\n{ key}={ value}");
break;
}
}
iniBlock.CustomData = iniData;
}
public void WriteData(string stringdata)
{
iniBlock.CustomData = stringdata;
}
public bool Fill&lt;T&gt;(string section, string key, ref T Variable, T defaultvalue, bool trim = true)
{
if (!Read&lt;T&gt;(section, key, ref Variable, defaultvalue, trim))
{
Write(section, key, Variable.ToString());
return false;
}
return true;
}
public bool Fill(string section, string key, ref Color Variable, Color defaultvalue, bool trim = true)
{
if (!Read(section, key, ref Variable, defaultvalue, trim))
{
Write(section, key, $"{Variable.R},{Variable.G},{Variable.B}");
return false;
}
return true;
}
}

