List&lt;T&gt; FillByGroup&lt;T&gt;(string name) where T : IMyTerminalBlock
{
var ret = new List&lt;T&gt;();
if (name != "")
{
IMyBlockGroup group = GridTerminalSystem.GetBlockGroupWithName(name);
if (group != null)
{
var tmplist = new List&lt;IMyTerminalBlock&gt;();
group.GetBlocks(tmplist);
foreach (IMyTerminalBlock tmp in tmplist)
{
if (tmp is T) ret.Add((T)tmp);
}
}
else Echo("Warning! Group '" + name + "' not found");
}
return ret;
}
List&lt;T&gt; FillByPrefix&lt;T&gt;(List&lt;IMyTerminalBlock&gt; alllist, string prefix) where T : IMyTerminalBlock
{
var ret = new List&lt;T&gt;();
if (prefix.Length &gt; 0)
{
foreach (IMyTerminalBlock block in alllist)
{
if (block.CustomName.Contains(prefix) &amp;&amp; block is T) ret.Add((T)block);
}
}
return ret;
}

List&lt;T&gt; DivideTvsOther&lt;T&gt;(ref List&lt;IMyFunctionalBlock&gt; alllist) where T : IMyFunctionalBlock
{
var ret = new List&lt;T&gt;();
for (int i = 0; i &lt; alllist.Count; i++)
{
if (alllist[i] is T)
{
ret.Add((T)alllist[i]);
alllist.RemoveAt(i);
i--;
}
}
return ret;
}

List&lt;IMyFunctionalBlock&gt; DivideOthervsTU&lt;T, U&gt;(List&lt;IMyFunctionalBlock&gt; alllist, ref List&lt;IMyFunctionalBlock&gt; tlist, ref List&lt;IMyFunctionalBlock&gt; ulist) where T : IMyFunctionalBlock where U : IMyFunctionalBlock
{
var ret = new List&lt;IMyFunctionalBlock&gt;();
foreach (IMyFunctionalBlock bl in alllist)
{
if (bl is T) tlist.Add((T)bl);
else if (bl is U) ulist.Add((U)bl);
else ret.Add(bl);
}
return ret;
}
void DivideGravsByDir(List&lt;IMyFunctionalBlock&gt; alllist, IMyTerminalBlock refblock, ref List&lt;IMyFunctionalBlock&gt;[] gravLists)
{
Vector3D meLeft = refblock.WorldMatrix.Left;
Vector3D meRight = refblock.WorldMatrix.Right;
Vector3D meUp = refblock.WorldMatrix.Up;
Vector3D meDown = refblock.WorldMatrix.Down;
Vector3D meForw = refblock.WorldMatrix.Forward;
Vector3D meBack = refblock.WorldMatrix.Backward;
foreach (IMyFunctionalBlock bl in alllist)
{
bl.ShowOnHUD = false;
Vector3D dir = bl.WorldMatrix.Up;
if (dir == meLeft) gravLists[1].Add(bl);
else if (dir == meRight) gravLists[2].Add(bl);
else if (dir == meUp) gravLists[3].Add(bl);
else if (dir == meDown) gravLists[4].Add(bl);
else if (dir == meForw) gravLists[5].Add(bl);
else if (dir == meBack) gravLists[6].Add(bl);
else bl.ShowOnHUD = true;
}
}
void DivideMassByDir(List&lt;IMyFunctionalBlock&gt; alllist, IMyTerminalBlock refblock, ref List&lt;IMyFunctionalBlock&gt;[] massLists)
{
const double c = 0.1;
double minX, maxX, minY, maxY, minZ, maxZ;
minX = minY = minZ = double.MaxValue;
maxX = maxY = maxZ = double.MinValue;
MatrixD refM = MatrixD.Invert(refblock.WorldMatrix);
foreach (IMyFunctionalBlock bl in alllist)
{
Vector3D pos = Vector3D.TransformNormal(bl.GetPosition(), refM);
minX = Math.Min(pos.X, minX);
maxX = Math.Max(pos.X, maxX);
minY = Math.Min(pos.Y, minY);
maxY = Math.Max(pos.Y, maxY);
minZ = Math.Min(pos.Z, minZ);
maxZ = Math.Max(pos.Z, maxZ);
}
foreach (IMyFunctionalBlock bl in massLists[0])
{
bl.ShowOnHUD = true;
Vector3D pos = Vector3D.TransformNormal(bl.GetPosition(), refM);
if (pos.X &lt; minX - c) { massLists[1].Add(bl); bl.ShowOnHUD = false; }
if (pos.X &gt; maxX + c) { massLists[2].Add(bl); bl.ShowOnHUD = false; }
if (pos.Y &gt; maxY + c) { massLists[3].Add(bl); bl.ShowOnHUD = false; }
if (pos.Y &lt; minY - c) { massLists[4].Add(bl); bl.ShowOnHUD = false; }
if (pos.Z &lt; minZ - c) { massLists[5].Add(bl); bl.ShowOnHUD = false; }
if (pos.Z &gt; maxZ + c) { massLists[6].Add(bl); bl.ShowOnHUD = false; }
}
}

