{
this.centralList = massList;
this.sideLists = gravLists;
this.sideLists[0] = UniteLists(gravLists);
gravLists[0] = this.sideLists[0];
Enabled = false;
Cruise = false;
SPM = false;
StartMoving += LinearGravMove;
StopMoving += LinearStop;
}
public override bool Enabled
{
get { return base.Enabled; }
set
{
base.Enabled = value;
if (!Enabled)
{
if (GGyros.Mode == DGyros.GMode.evasion) GGyros.ToggleEvasion();
GThrusters.ToggleEvasion(value);
SetBlocksOnOff(centralList, false);
SetBlocksOnOff(sideLists[0], false);
}
}
}
public override bool SPM
{
get { return isSPM; }
set
{
isSPM = value;
CurrPower = isSPM ? slowPowerLvl : G;
if (isSPM) SetGravField(sideLists[0], slowFieldSize);
else SetGravField(sideLists[0], maxFieldSile);
}
}
private void LinearGravMove(int dir, float pow)
{
SetBlocksOnOff(centralList, true);
foreach (IMyGravityGenerator bl in sideLists[dir])
{
bl.GravityAcceleration = -pow;
bl.Enabled = true;
}
foreach (IMyGravityGenerator bl in sideLists[invdir[dir]])
{
bl.GravityAcceleration = pow;
bl.Enabled = true;
}
}
private void LinearStop()
{
SetBlocksOnOff(centralList, false);
SetBlocksOnOff(sideLists[0], false);
}
private void SetGravField(List&lt;IMyFunctionalBlock&gt; gravlist, float size)
{
foreach (IMyGravityGenerator bl in gravlist)
{
Vector3 tmpv = new Vector3(size, size, size);
bl.FieldSize = tmpv;
}
}
}
public class DThrusters
{
public List&lt;IMyThrust&gt;[] Thrustrs = new List&lt;IMyThrust&gt;[7];
protected bool useEvasionThrust = true;
protected bool disableSideThrust = true;
protected bool isActive = false;
protected Dictionary&lt;bool, float&gt; orr = new Dictionary&lt;bool, float&gt; { [false] = 0, [true] = 1 };
public DThrusters(IMyTerminalBlock me, bool useEvasionThrust, bool disableSideThrust)
{
Thrustrs = new List&lt;IMyThrust&gt;[7];
for (int i = 0; i &lt; 7; i++) Thrustrs[i] = new List&lt;IMyThrust&gt;();
var tmpAllTRs = new List&lt;IMyThrust&gt;();
GTS.GetBlocksOfType&lt;IMyThrust&gt;(tmpAllTRs);
foreach (IMyThrust tr in tmpAllTRs) { if (tr.IsSameConstructAs(me)) Thrustrs[0].Add(tr); }
foreach (IMyThrust tr in Thrustrs[0])
{
Vector3I dir = tr.GridThrustDirection;
int d;
if (dir.Equals(Vector3I.Left)) d = 1;
else if (dir.Equals(Vector3I.Right)) d = 2;
else if (dir.Equals(Vector3I.Up)) d = 3;
else if (dir.Equals(Vector3I.Down)) d = 4;
else if (dir.Equals(Vector3I.Forward)) d = 5;
else if (dir.Equals(Vector3I.Backward)) d = 6;
else d = -1;
if (d &gt;= 0) Thrustrs[d].Add(tr);
}
this.useEvasionThrust = useEvasionThrust;
this.disableSideThrust = disableSideThrust;
}
public void ToggleEvasion(bool onoff)
{
if (isActive != onoff)
{
if (useEvasionThrust) SetThrustOverride(Thrustrs[4], onoff);
if (disableSideThrust)
{
SetThrustOnOff(Thrustrs[1], !onoff);
SetThrustOnOff(Thrustrs[2], !onoff);
SetThrustOnOff(Thrustrs[3], !onoff);
}
isActive = onoff;
}
}
void SetThrustOnOff(List&lt;IMyThrust&gt; thrustList, bool onoff)
{
foreach (IMyThrust bl in thrustList) bl.Enabled = onoff;
}
protected void SetThrustOverride(List&lt;IMyThrust&gt; thrustList, bool onoff)
{
foreach (IMyThrust th in thrustList) th.ThrustOverridePercentage = orr[onoff];
}
}
public class DGyros
{
private readonly List&lt;GyroDefinition&gt; gyroDefs;
protected readonly bool hasGyros = false;
protected bool isGyrosActive = false;
protected readonly float gyroStabPow, gyroEvasionPow;
protected readonly float Roll, Pitch, Yaw;
public enum GMode { off, stab, evasion }
protected GMode mode;
protected bool isActiveStab = false;
public GMode Mode { get { return mode; } }
public DGyros(IMyShipController orientCockpit, List&lt;IMyGyro&gt; gyrolist, int pow1, int pow2, float roll, float pitch, float yaw)
{
gyroDefs = new List&lt;GyroDefinition&gt;();
SetupGyros(orientCockpit, gyrolist, gyroDefs);
this.gyroStabPow = pow1 / 100f;
this.gyroEvasionPow = pow2 / 100f;
this.Roll = roll / 9.55f;
this.Pitch = pitch / 9.55f;
this.Yaw = yaw / 9.55f;
if (gyroDefs.Count &gt; 0) hasGyros = true;
this.mode = GMode.off;
}
public void ToggleStab()
{
if (hasGyros)
{
if (mode != GMode.stab) mode = GMode.stab;
else mode = GMode.off;
}
}
public void ToggleEvasion()
{
if (hasGyros)
{
if (mode != GMode.evasion)
{
mode = GMode.evasion;
foreach (GyroDefinition bl in gyroDefs)
{
bl.Gyro.GyroPower = gyroEvasionPow;
SetGyroParams(bl, true, Pitch, Yaw, Roll);
}
}
else
{
mode = GMode.off;
foreach (GyroDefinition bl in gyroDefs)
{
bl.Gyro.GyroPower = 1f;
SetGyroParams(bl, false, 0f, 0f, 0f);
}
}
}
}
public void ProcessGyrosStab(bool onoff)
{
if (hasGyros &amp;&amp; mode == GMode.stab)
{
if (!isActiveStab == onoff)
{
foreach (GyroDefinition bl in gyroDefs)
{
if (onoff) bl.Gyro.GyroPower = gyroStabPow;
SetGyroParams(bl, onoff, 0f, 0f, 0f);
}
isActiveStab = onoff;
}
}
}

private enum GyroRotations { Pitch, Yaw, Roll }
private class GyroDefinition
{
public IMyGyro Gyro;
public float PitchFactor = 1f;
public float YawFactor = 1f;
public float RollFactor = 1f;
public GyroRotations Pitch = GyroRotations.Pitch;
public GyroRotations Yaw = GyroRotations.Yaw;
public GyroRotations Roll = GyroRotations.Roll;
public GyroDefinition(IMyGyro mGyro)
{
Gyro = mGyro;
}
}
private static void SetupGyros(IMyShipController OrientationCockpit, List&lt;IMyGyro&gt; GyrosList, List&lt;GyroDefinition&gt; GyroDefsList)
{
Matrix cockpitMatrix, orientMatrix;
OrientationCockpit.Orientation.GetMatrix(out cockpitMatrix);
GyroDefinition GyroDef;
foreach (IMyGyro gyro in GyrosList)
{
gyro.Orientation.GetMatrix(out orientMatrix);
GyroDef = new GyroDefinition(gyro);
if (orientMatrix.Forward == cockpitMatrix.Forward)
{
GyroDef.Roll = GyroRotations.Roll;
GyroDef.RollFactor = 1f;
if (orientMatrix.Up == cockpitMatrix.Up)
{
GyroDef.Pitch = GyroRotations.Pitch;
GyroDef.PitchFactor = 1f;
GyroDef.Yaw = GyroRotations.Yaw;
GyroDef.YawFactor = 1f;
}
else if (orientMatrix.Up == cockpitMatrix.Right)
{
GyroDef.Pitch = GyroRotations.Yaw;
GyroDef.PitchFactor = 1f;
GyroDef.Yaw = GyroRotations.Pitch;
GyroDef.YawFactor = -1f;
}
else if (orientMatrix.Up == cockpitMatrix.Down)
{
