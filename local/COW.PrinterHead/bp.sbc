public class CommonGDrive
{
private bool enabled = false;
protected List&lt;IMyShipController&gt; ctrlList;
protected IMyShipController CurrController;
protected bool isPressd = false;
protected bool isCruise = false;
protected bool isSPM = false;
protected bool isStab = false;
protected bool isEvasion = false;
protected readonly float maxFieldSile;
protected readonly float slowPowerLvl;
protected float CurrPower { get; set; }
protected readonly float slowFieldSize;
public const float G = 9.810f;
public const float Gdiv25 = 0.3924f;
protected List&lt;IMyFunctionalBlock&gt; centralList;
protected List&lt;IMyFunctionalBlock&gt;[] sideLists = new List&lt;IMyFunctionalBlock&gt;[7];
protected Dictionary&lt;int, int&gt; invdir =
new Dictionary&lt;int, int&gt; { [1] = 2, [2] = 1, [3] = 4, [4] = 3, [5] = 6, [6] = 5 };
protected delegate void MoveHandler(int dir, float pow);
protected event MoveHandler StartMoving;
protected delegate void StopHandler();
protected event StopHandler StopMoving;
public DThrusters GThrusters;
public DGyros GGyros;
public CommonGDrive(List&lt;IMyShipController&gt; ctrls, float slowpow, float maxFieldSile, float slowFieldSize, DThrusters gThrusters, DGyros gGyros)
{
this.ctrlList = ctrls;


this.slowPowerLvl = slowpow;
this.maxFieldSile = maxFieldSile;
this.slowFieldSize = slowFieldSize;
this.GThrusters = gThrusters;
this.GGyros = gGyros;
}
public virtual bool Enabled
{
get { return enabled; }
set
{
enabled = value;
CurrController = GetShipController(ctrlList);
}
}
public virtual bool Cruise
{
get { return isCruise; }
set
{
isCruise = value;
SetBlocksOnOff(GThrusters.Thrustrs[5], !isCruise);
}
}
public virtual bool SPM
{
get { return isSPM; }
set { isSPM = value; }
}
public virtual bool Stab
{
get { return isStab; }
set
{
isStab = value;
GGyros.ToggleStab();
}
}
public virtual bool Evasion
{
get { return isEvasion; }
