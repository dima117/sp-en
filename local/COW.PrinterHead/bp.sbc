GravName=Gravi
// Search for blocks by group or prefix in the block name
IsGroup=true
// Is the gravity drive type on spherical generators or not?
IsSphrerical=false
// The name of the group/prefix that will be disabled when the engine is running
DisableName=
// Auto-off when the pilot leaves the cockpit
AutoOff=true
// The minimum flight speed at which the gravity engine operates
MinSpeed=10
// Field size of generators in maximum power mode
MaxFieldSize=400
// Field size of generators in reduced power mode
SlowFieldSize=40
// Gravity drive power in reduced power mode as a percentage of the maximum (0 - 100
SlowPowerLvl=5
// Power of gyroscopes used for motion stabilization (0 - 100)
GyroStabPower=20

[Evasion]
// Using tracers in evasion mode
UseEvasionThrust=true
// Disabling side tracers in evasion mode
DisableSideThrust=true
// Rotate per minute (RPM) for gyroscopes of rotation on axes in the evasion mode
Roll=4
Pitch=0
Yaw=0
// The power of gyroscopes of rotation in the evasion mode (0 - 100)
GyroEvasionPower=30

[Display]
// Prefix for screens that will display the mode of operation of the gravity engine
Screen=-1
// Interface colors
MainColor=220,80,0
OnColor=255,0,0
BackgroundColor=0,0,0


*/


const string Version = "1.0.0";
IMyTextSurface PB_LCD;
INI Ini;
static IMyGridTerminalSystem GTS;
bool isSetup = false;
bool AutoOff = true;
bool UseLCD = false;
StringBuilder EchoStr, InfoStr;
int Tick, PilotsCount, MinSpeed, Screen;
Color BckColor = new Color(0, 0, 0);
Color MainCol = new Color(220, 80, 0);
Color OnCol = new Color(255, 0, 0);
List&lt;IMyShipController&gt; Cockpits;
List&lt;IMyTextSurface&gt; CockpitSurfs;
List&lt;IMyTextPanel&gt; LCDs;
CommonGDrive GDrive;
readonly Dictionary&lt;int, string&gt; DispGrav = new Dictionary&lt;int, string&gt;()
{ {0, @"   G" },  {1, @"  GR" },  {2, @" GRA" },  {3, @"GRAV" },  {4, @"RAV  " }, {5, @"AV    " },  {6, @"V     " }, {7, @"        " } };
List&lt;IMyFunctionalBlock&gt; OnList;
List&lt;IMyFunctionalBlock&gt; DisableList;
bool EvenTick = false;
readonly Dictionary&lt;int, string&gt; Dirs = new Dictionary&lt;int, string&gt;
{ [1] = "Left", [2] = "Right", [3] = "Up", [4] = "Down", [5] = "Forward", [6] = "Backward" };

Program()
{
Setup();
}
public void Main(string arg)
{
if (isSetup)
{
arg = arg.ToLower();
string[] ar = arg.Split('-');
bool isToggle = true;
bool wantOn = false;
if (ar.Length &gt; 1)
{
if (ar[1] == "on")
{
isToggle = false;
wantOn = true;
}
else if (ar[1] == "off") { isToggle = false; }
}
switch (ar[0])
{
case "grav":
if (isToggle) OnOffGDrive(!GDrive.Enabled);
else OnOffGDrive(wantOn);
break;
case "cruise":
if (isToggle) GDrive.Cruise = !GDrive.Cruise;
else GDrive.Cruise = wantOn;
break;
case "slow":
if (isToggle) GDrive.SPM = !GDrive.SPM;
else GDrive.SPM = wantOn;
break;
case "stab":
if (isToggle) GDrive.Stab = !GDrive.Stab;
else GDrive.Stab = wantOn;
break;
case "evasion":
if (!GDrive.Evasion) OnOffGDrive(true);
if (isToggle) GDrive.Evasion = !GDrive.Evasion;
else GDrive.Evasion = wantOn;
break;
case "setup":
Setup();
break;
}
if (GDrive.Enabled)
{
UpdateGravDrive();
if (AutoOff)
{
int cnt = CalcPilotsCount();
if (cnt &lt; PilotsCount) OnOffGDrive(false);
PilotsCount = cnt;
}
}
ShowInfo();
EvenTick = !EvenTick;
}
else Setup();
}
void ShowInfo()
{
EchoStr.Clear();
InfoStr.Clear();
if (GDrive.Cruise)
{
EchoStr.Append("CR     ");
InfoStr.Append("Cruise     ");
}
else
{
EchoStr.Append("    ");
InfoStr.Append("           ");
}
if (GDrive.SPM)
{
EchoStr.Append("Slow");
InfoStr.Append("Slow      ");
}
else InfoStr.Append("          ");
if (GDrive.GGyros.Mode == DGyros.GMode.stab)
{
EchoStr.Append("Gyro");
InfoStr.Append("Stab");
}
else if (GDrive.GGyros.Mode == DGyros.GMode.evasion)
{
if (EvenTick)
{
EchoStr.Append("Evas");
InfoStr.Append("Evas");
}
else InfoStr.Append("    ");
}
else InfoStr.Append("    ");
if (GDrive.Enabled) InfoStr.Append("\n-------- GRAV ON --------");
else InfoStr.Append("\n GRAV OFF");
Echo(EchoStr.ToString());
foreach (IMyTextSurface lcd in CockpitSurfs) { if (lcd != null) lcd.WriteText(InfoStr.ToString()); }
if (UseLCD) foreach (IMyTextPanel lcd in LCDs) { lcd.WriteText(InfoStr.ToString()); }
}
void OnOffGDrive(bool onoff)
{
if (onoff)
{
foreach (IMyTextSurface lcd in CockpitSurfs) { if (lcd != null) lcd.FontColor = OnCol; }
if (UseLCD) foreach (IMyTextPanel lcd in LCDs) { lcd.FontColor = OnCol; }
Runtime.UpdateFrequency = UpdateFrequency.Update10;
}
else
{
foreach (IMyTextSurface lcd in CockpitSurfs) { if (lcd != null) lcd.FontColor = MainCol; }
if (UseLCD) foreach (IMyTextPanel lcd in LCDs) { lcd.FontColor = MainCol; }
GDrive.Evasion = false;
Runtime.UpdateFrequency = UpdateFrequency.None;
}
SetBlocksOnOff(OnList, onoff);
SetBlocksOnOff(DisableList, !onoff);
GDrive.Enabled = onoff;
}
int CalcPilotsCount()
{
int tmp = 0;
foreach (IMyShipController ck in Cockpits) if (ck.IsUnderControl) tmp++;
return tmp;
}
void UpdateGravDrive()
{
GDrive.MoveFromCockpit();
GDrive.EvasionMove();
GDrive.DamperMove(MinSpeed);
Echo(DispGrav[Tick]);
Tick++;
if (Tick &gt; 7) Tick = 0;
}
void SetBlocksOnOff(List&lt;IMyFunctionalBlock&gt; blockList, bool onoff)
{
foreach (IMyFunctionalBlock bl in blockList) bl.Enabled = onoff;
}
bool CheckCockpits()
{
foreach (IMyShipController ck in Cockpits) if (ck.IsUnderControl) return true;
return false;
}
void Setup()
{
Echo("Running setup ...");
isSetup = true;
Ini = new INI(Me);
GTS = GridTerminalSystem;

const string s1 = "Used default ";
string sec = "GDrive";
if (!Ini.FindSection(sec)) Ini.AddSection(sec);
int gyroStabPower = 20;
if (!Ini.Fill(sec, "GyroStabPower", ref gyroStabPower, gyroStabPower)) Echo($"{s1} [{sec}] 'GyroStabPower'");
int slowPowerLvl = 5;
if (!Ini.Fill(sec, "SlowPowerLvl", ref slowPowerLvl, slowPowerLvl)) Echo($"{s1} [{sec}] 'SlowPowerLvl'");
int slowFieldSize = 40;
if (!Ini.Fill(sec, "SlowFieldSize", ref slowFieldSize, slowFieldSize)) Echo($"{s1} [{sec}] 'SlowFieldSize'");
int maxFieldSile = 400;
if (!Ini.Fill(sec, "MaxFieldSize", ref maxFieldSile, maxFieldSile)) Echo($"{s1} [{sec}] 'MaxFieldSile'");
if (!Ini.Fill(sec, "MinSpeed", ref MinSpeed, 10)) Echo($"{s1} [{sec}] 'MinSpeed'");
if (!Ini.Fill(sec, "AutoOff", ref AutoOff, true)) Echo($"{s1} [{sec}] 'AutoOff'");
string disableName = "";
if (!Ini.Fill(sec, "DisableName", ref disableName, disableName)) Echo($"{s1} [{sec}] 'DisableName'");
bool isSphrerical = false;
if (!Ini.Fill(sec, "IsSphrerical", ref isSphrerical, isSphrerical)) Echo($"{s1} [{sec}] 'IsSphrerical'");
bool isGroup = true;
if (!Ini.Fill(sec, "IsGroup", ref isGroup, isGroup)) Echo($"{s1} [{sec}] 'IsGroup'");
string gdriveName = "Gravi";
if (!Ini.Fill(sec, "GravName", ref gdriveName, gdriveName)) Echo($"{s1} [{sec}] 'GravName'");
sec = "Evasion";
if (!Ini.FindSection(sec)) Ini.AddSection(sec);
int gyroEvasionPower = 30;
if (!Ini.Fill(sec, "GyroEvasionPower", ref gyroEvasionPower, gyroEvasionPower)) Echo($"{s1} [{sec}] 'GyroEvasionPower'");
float yaw = 0;
if (!Ini.Fill(sec, "Yaw", ref yaw, yaw)) Echo($"{s1} [{sec}] 'Yaw'");
float pitch = 0;
if (!Ini.Fill(sec, "Pitch", ref pitch, pitch)) Echo($"{s1} [{sec}] 'Pitch'");
float roll = 4;
if (!Ini.Fill(sec, "Roll", ref roll, roll)) Echo($"{s1} [{sec}] 'Roll'");
bool disableSideThrust = true;
if (!Ini.Fill(sec, "DisableSideThrust", ref disableSideThrust, disableSideThrust)) Echo($"{s1} [{sec}] 'DisableSideThrust'");
bool useEvasionThrust = true;
if (!Ini.Fill(sec, "UseEvasionThrust", ref useEvasionThrust, useEvasionThrust)) Echo($"{s1} [{sec}] 'UseEvasionThrust'");
sec = "Display";
if (!Ini.FindSection(sec)) Ini.AddSection(sec);
if (!Ini.Fill(sec, "BackgroundColor", ref BckColor, BckColor)) Echo($"{s1} [{sec}] 'BackgroundColor'");
if (!Ini.Fill(sec, "OnColor", ref OnCol, OnCol)) Echo($"{s1} [{sec}] 'OnColor'");
if (!Ini.Fill(sec, "MainColor", ref MainCol, MainCol)) Echo($"{s1} [{sec}] 'MainColor'");
if (!Ini.Fill(sec, "Screen", ref Screen, -1)) Echo($"{s1} [{sec}] 'Screen'");

var tmpBlocks = new List&lt;IMyTerminalBlock&gt;();
GridTerminalSystem.GetBlocks(tmpBlocks);
List&lt;IMyFunctionalBlock&gt; tmpFuncBlocks;
List&lt;IMyGyro&gt; gyrosBlockList;
OnList = new List&lt;IMyFunctionalBlock&gt;();
DisableList = new List&lt;IMyFunctionalBlock&gt;();
if (isGroup)
{
DisableList = FillByGroup&lt;IMyFunctionalBlock&gt;(disableName);
tmpFuncBlocks = FillByGroup&lt;IMyFunctionalBlock&gt;(gdriveName);
}
else
{
DisableList = FillByPrefix&lt;IMyFunctionalBlock&gt;(tmpBlocks, disableName);
tmpFuncBlocks = FillByPrefix&lt;IMyFunctionalBlock&gt;(tmpBlocks, gdriveName);
}

EchoStr = new StringBuilder(40);
InfoStr = new StringBuilder(40);
if (isGroup)
Cockpits = FillByGroup&lt;IMyShipController&gt;(gdriveName);
else
Cockpits = FillByPrefix&lt;IMyShipController&gt;(tmpBlocks, gdriveName);
CockpitSurfs = new List&lt;IMyTextSurface&gt;();
IMyShipController xcurrCockpit = null;
if (Cockpits.Count &gt; 0)
{
foreach (IMyShipController ck in Cockpits)
{
IMyTextSurface lcd;
lcd = (ck as IMyTextSurfaceProvider)?.GetSurface(Screen);
if (lcd != null)
{
lcd.ContentType = VRage.Game.GUI.TextPanel.ContentType.TEXT_AND_IMAGE;
lcd.WriteText("");
lcd.FontColor = MainCol;
lcd.BackgroundColor = BckColor;
lcd.FontSize = 1f;
lcd.Font = "Monospace";
lcd.TextPadding = 2f;
lcd.Alignment = TextAlignment.CENTER;
CockpitSurfs.Add(lcd);
}
}
xcurrCockpit = CommonGDrive.GetShipController(Cockpits);
Echo("Cockpits Count: " + Cockpits.Count.ToString());
}
else
{
Echo("Error!!! Cockpits in '" + gdriveName + "' not found");
isSetup = false;
return;
}
LCDs = DivideTvsOther&lt;IMyTextPanel&gt;(ref tmpFuncBlocks);
if (LCDs.Count &gt; 0)
{
foreach (IMyTextPanel lcd in LCDs)
{
lcd.ContentType = VRage.Game.GUI.TextPanel.ContentType.TEXT_AND_IMAGE;
lcd.WriteText("");
lcd.FontColor = MainCol;
lcd.BackgroundColor = BckColor;
